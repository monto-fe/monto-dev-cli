{
  "name": "dux-react-dev-cli",
  "version": "0.0.1",
  "description": "Cli tool for react app dev",
  "author": "heng.du <853788089@qq.com>",
  "license": "MIT",
  "type": "commonjs",
  "bin": {
    "react-dev-cli": "index.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "main": "index.js",
  "files": [
    "index.js",
    "config.js",
    "command",
    "lib",
    "USAGE.md"
  ],
  "scripts": {
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "docs:update": "node scripts/docs.js",
    "publish:patch": "npm version patch && npm publish --registry=http://registry.npm.pre.ucloudadmin.com"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "chalk": "^2.4.2",
    "chokidar": "^3.3.1",
    "enquirer": "^2.3.2",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "is-git-clean": "^1.1.0",
    "jscodeshift": "^0.12.0",
    "ncp": "^2.0.0",
    "ora": "^5.4.1",
    "qs": "^6.9.4",
    "yargs": "^13.2.4"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-node": "^9.1.0",
    "lint-staged": ">=10",
    "prettier": "^2.3.2",
    "yorkie": "^2.0.0"
  }
}
